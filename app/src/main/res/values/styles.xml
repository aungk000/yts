<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextWithIcon">
        <item name="android:drawablePadding">@dimen/dimen_large</item>
        <item name="android:padding">@dimen/dimen_normal</item>
        <item name="android:textAlignment">textEnd</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginStart">@dimen/dimen_normal</item>
        <item name="android:layout_marginEnd">@dimen/dimen_normal</item>
    </style>
    <style name="ShapeCircle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="ShapeRoundCorner">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>
    <style name="MaterialAlertDialog" parent="ThemeOverlay.Material3.MaterialAlertDialog">
        <!--<item name="dialogCornerRadius">@dimen/dialog_corner</item>-->
        <item name="android:windowBackground">@drawable/dialog_inset</item>
    </style>
</resources>